import java.util.Scanner;

public class FormingMagicSquare {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int s[][]=new int[3][3];
		int firstrow=0;
		int secondrow=0;
		int thirdrow=0;
		for(int i=0;i<3;i++) {
			for(int j=0;j<3;j++) {
				if(i==0) {
					firstrow+=sc.nextInt();
				}else if(i==1) {
					secondrow+=sc.nextInt();
				}
				else {
					thirdrow+=sc.nextInt();
				}
				
			}
		}
		int firstrwd= firstrow>15 ? firstrow-15: 15-firstrow;
		int secondrwd= firstrow>15 ? firstrow-15: 15-firstrow;
		int thirdrwd= firstrow>15 ? firstrow-15: 15-firstrow;

      		
		
	}

}
