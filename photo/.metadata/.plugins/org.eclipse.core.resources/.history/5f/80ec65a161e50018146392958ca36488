import java.util.Scanner;

public class FormingMagicSquare {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int s[][]=new int[3][3];
		for(int i=0;i<3;i++) {
			for(int j=0;j<3;j++) {
				s[i][j]=sc.nextInt();
			}
		}
		int a[]= {8,4,3};
		int b[]= {8,1,6};
		int c[]= {9,5,1};
		int d[]= {9,4,2};
		int e[]= {7,5,3};
		int f[]= {7,6,2};
		
		
		
	}

}
