import java.util.Scanner;

public class SuperiorString {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int t=sc.nextInt();
		while(t-->0) {
			int n=sc.nextInt();
			String s=sc.next();
			int a[][]=new int[n][26];
			int b[]=new int[26];
			for(int i=0;i<n;i++) {
				
				b[s.charAt(i)-97]++;
				a[i]=b;
			}
		}
	}

}
